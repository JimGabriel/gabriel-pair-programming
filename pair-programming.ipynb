    "#change cordinates from polar to cartesion\n",
    "import math \n",
    "def polar_to_cart(r, theta):\n",
    "    x = r * math.cos(theta)\n",
    "    y = r * math.sin(theta)\n",
    "    return(x, y)\n",
    "\n"
